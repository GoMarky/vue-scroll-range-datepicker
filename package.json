    "sass-loader": "^13.2.0",
    "serve-static": "^1.13.2",
    "vue-loader": "^17.0.1",
    "vue-template-compiler": "^2.6.10",
    "vuex": "3.0.1",
    "webpack": "^3.11.0",
    "webpack-cli": "^2.1.4",
    "webpack-dev-server": "^2.11.2",
    "webpack-merge": "^1.1.2"
  },
  "dependencies": {
    "date-fns": "^1.29.0",
    "vue": "^2.6.10"
  }
