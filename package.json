{
  "name": "vue-scroll-range-datepicker",
  "description": "A Vue.js Plugin",
  "version": "3.0.7",
  "author": {
    "name": "Andrew",
    "email": "swen295@gmail.com"
  },
  "keywords": [
    "vue",
    "vuejs",
    "plugin"
  ],
  "license": "MIT",
  "main": "dist/vue-scroll-range-datepicker.common.js",
  "unpkg": "dist/vue-scroll-range-datepicker.browser.js",
  "module": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack --config config/webpack.config.dev.js --progress --watch --hot",
    "build": "npm run build:browser && npm run build:common",
    "build:browser": "cross-env NODE_ENV=production webpack --config config/webpack.config.browser.js --progress --hide-modules",
    "build:common": "cross-env NODE_ENV=production webpack --config config/webpack.config.common.js --progress --hide-modules",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teodorDre/vue-scroll-range-datepicker.git"
  },
  "bugs": {
